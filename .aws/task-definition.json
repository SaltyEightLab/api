{
    "containerDefinitions": [
      {
        "name": "web",
        "image": "<image>",
        "cpu": 0,
        "memoryReservation": 512,
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "entryPoint": [],
        "command": [],
        "environment": [],
        "mountPoints": [],
        "volumesFrom": [],
        "dependsOn": [
          {
            "containerName": "api",
            "condition": "HEALTHY"
          }
        ],
        "readonlyRootFilesystem": false
      },

      {
        "name": "api",
        "image": "<image>",
        "cpu": 0,
        "memoryReservation": 512,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "entryPoint": [],
        "command": [],
        "environment": [],
        "mountPoints": [],
        "volumesFrom": [],
        "dependsOn": [
          {
            "containerName": "db",
            "condition": "HEALTHY"
          }
        ],
        "healthCheck": {
        "command": [
            "CMD-SHELL",
            "curl -f http://localhost:8080/api/hello?lang=en || exit 1"
            ],
          "interval": 10,
          "timeout": 5,
          "retries": 5,
          "startPeriod": 10
        }
      },

      {
        "name": "db",
        "image": "<image>",
        "cpu": 0,
        "memoryReservation": 512,
        "portMappings": [
          {
            "containerPort": 5432,
            "hostPort": 5432,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "environment": [
          {
            "name": "POSTGRES_PASSWORD",
            "value": "Yashi0Takuy@"
          },
          {
            "name": "POSTGRES_USER",
            "value": "postgres"
          },
          {
            "name": "POSTGRES_DB",
            "value": "appdb"
          }
        ],
        "healthCheck": {
        "command": [
            "CMD-SHELL",
            "pg_isready -U postgres -d appdb -t 5 || exit 1"
            ],
          "interval": 10,
          "timeout": 5,
          "retries": 3,
          "startPeriod": 10
        }
      }
    ],

    "family": "sacicd-def",
    "executionRoleArn": "ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "status": "ACTIVE",
    "requiresAttributes": [
      {
        "name": "com.amazonaws.ecs.capability.ecr-auth"
      },
      {
        "name": "com.amazonaws.ecs.capability.docker-remote-api.1.19"
      },
      {
        "name": "com.amazonaws.ecs.capability.docker-remote-api.1.21"
      },
      {
        "name": "ecs.capability.container-ordering"
      },
      {
        "name": "ecs.capability.container-health-check"
      },
      {
        "name": "ecs.capability.execution-role-ecr-pull"
      },
      {
        "name": "com.amazonaws.ecs.capability.docker-remote-api.1.18"
      },
      {
        "name": "ecs.capability.task-eni"
      },
      {
        "name": "com.amazonaws.ecs.capability.docker-remote-api.1.29"
      }
    ],
    "placementConstraints": [],
    "compatibilities": ["EC2", "FARGATE"],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "1024"
  }